{
  "name": "user.api",
  "version": "1.0.0",
  "description": "A REST API for User CRUD operations written in .Net with an ElasticSearch backend",
  "main": "index.js",
  "scripts": {
    "aws:login": "npm run cdk bootstrap aws://000000000000/us-east-1",
    "cdk": "cdklocal",
    "cdk:synth": "cd cdk && cdklocal synth",
    "cdk:synth:write": "cd cdk && cdklocal synth > cdk.out/template.yml",
    "cdk:deploy": "cd cdk && cdk deploy",
    "clean": "rm -rf cdk/cdk.out/* node_modules/* node_modules/.bin node_modules/.package-lock.json .cache",
    "clean:cdk": "rm -rf cdk/cdk.out/asset.*",
    "publish:image": "npm run aws:login && node ./scripts/publish-to-docker-registry.js",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.107",
    "@types/aws-lambda": "^8.10.133",
    "@types/node": "^20.11.10",
    "@types/uuid": "^9.0.8",
    "aws-cdk-lib": "^2.124.0",
    "aws-sdk": "^2.1607.0",
    "constructs": "^10.2.59",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "dependencies": {},
  "author": "",
  "license": "ISC"
}
